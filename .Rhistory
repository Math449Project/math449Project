predictor2=as.numeric(predictions5)-1
rocobj4 <- roc(response2, predictor2)
plot(rocobj4,print.auc=TRUE,print.auc.x=0.5,print.auc.y=0.2)
aucScore4=auc(rocobj4)
print(aucScore4)
